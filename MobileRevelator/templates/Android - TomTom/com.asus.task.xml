<?xml version='1.0' encoding='utf-8' standalone='yes' ?>
<Items>
<Timeline path="com.asus.task/databases/task.db" type="database">
        <sql cmd="select rowid, title, subtitle, description, info_reminder_time_millis from taskinfo;">
            <type text="Asus Task" category="Documents"/>
            <package>
                <field addfields="Title:&quot;{%1}&quot;;"/>
                <field addfields="Subtitle:&quot;{%2}&quot;"/>
            </package>
            <timestamp field="{%4}" int="/1000"/>
            <duration/>
            <desc/>
            <message>
                <field addfields="{%3}"/>
            </message>
            <contact_alias/>
            <contact/>
        </sql>
</Timeline>
</Items>