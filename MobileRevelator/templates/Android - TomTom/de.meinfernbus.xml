            <type text="Mein Fernbus [my_trips]" category="Locations"/>
            <package/>
            <timestamp field="{%9}" int=""/>
            <duration/>
            <desc/>
            <location>
                <field latitude="{%3}" longitude="{%4}"/>
                <field addfields=";Route to:{%1}"/>
                <field addfields=", {%2}"/>
                <field latitude="{%7}" longitude="{%8}"/>
                <field addfields=";Route from:{%5}"/>
                <field addfields=", {%6}"/>
            </location>
            <contact/>
            <contact_alias/>
            <message/>
        </sql>
</Timeline>
