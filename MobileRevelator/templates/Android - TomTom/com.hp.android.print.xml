            <type text="HP Print Job" category="Media"/>
            <package/>
            <timestamp field="{%1}" int="/1000"/>
            <duration/>
            <desc>
                <field addfields="Status: {%2};"/>
                <field addfields="Printername: {%3};"/>
                <field addfields="Printermodel/IP: {%4};"/>
            </desc>
            <contact/>
            <contact_alias/>
            <message/>
            <image/>
        </sql>
</Timeline>
</Items>